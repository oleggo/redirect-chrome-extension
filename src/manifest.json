{
  "name": "Redirect",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Redirect HTTP(S) requests based on rules",
  "author": "Oleg Gordeev",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "setup.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
